{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {
        "baseURL": "localhost",
        "port": 8080,
        "ssl": {
            "enabled": false,
            "keyPath": "",
            "certPath": ""
        },
        "paypal": {
            "useSandbox": false,
            "currency": "USD"
        },
        "tracker": {
            "name": "Donation Tracker",
            "databaseURL": "mongodb://localhost:27017",
            "homepage": "https://google.com",
            "logoPath": "",
            "passwordHash": "",
            "tokens": []
        },
        "legal": {
            "privacyPolicy": "",
            "sweepstakesRules": ""
        },
        "frontend": {
            "useDefault": true,
            "donationSuccessMessage": "Thanks for donating! Enjoy the event!"
        }
    },
    "title": "Root Schema",
    "required": [
        "baseURL",
        "port",
        "ssl",
        "paypal",
        "tracker",
        "legal",
        "frontend"
    ],
    "properties": {
        "baseURL": {
            "type": "string",
            "default": "",
            "title": "The baseURL Schema",
            "examples": [
                "localhost"
            ]
        },
        "port": {
            "type": "integer",
            "default": 0,
            "title": "The port Schema",
            "examples": [
                8080
            ]
        },
        "ssl": {
            "type": "object",
            "default": {},
            "title": "The ssl Schema",
            "required": [
                "enabled",
                "keyPath",
                "certPath"
            ],
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "default": false,
                    "title": "The enabled Schema",
                    "examples": [
                        false
                    ]
                },
                "keyPath": {
                    "type": "string",
                    "default": "",
                    "title": "The keyPath Schema",
                    "examples": [
                        ""
                    ]
                },
                "certPath": {
                    "type": "string",
                    "default": "",
                    "title": "The certPath Schema",
                    "examples": [
                        ""
                    ]
                }
            },
            "examples": [
                {
                    "enabled": false,
                    "keyPath": "",
                    "certPath": ""
                }
            ]
        },
        "paypal": {
            "type": "object",
            "default": {},
            "title": "The paypal Schema",
            "required": [
                "useSandbox",
                "currency"
            ],
            "properties": {
                "useSandbox": {
                    "type": "boolean",
                    "default": false,
                    "title": "The useSandbox Schema",
                    "examples": [
                        false
                    ]
                },
                "currency": {
                    "type": "string",
                    "default": "",
                    "title": "The currency Schema",
                    "examples": [
                        "USD"
                    ]
                }
            },
            "examples": [
                {
                    "useSandbox": false,
                    "currency": "USD"
                }
            ]
        },
        "tracker": {
            "type": "object",
            "default": {},
            "title": "The tracker Schema",
            "required": [
                "name",
                "databaseURL",
                "homepage",
                "logoPath",
                "passwordHash",
                "tokens"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "default": "",
                    "title": "The name Schema",
                    "examples": [
                        "Donation Tracker"
                    ]
                },
                "databaseURL": {
                    "type": "string",
                    "default": "",
                    "title": "The databaseURL Schema",
                    "examples": [
                        "mongodb://localhost:27017"
                    ]
                },
                "homepage": {
                    "type": "string",
                    "default": "",
                    "title": "The homepage Schema",
                    "examples": [
                        "https://google.com"
                    ]
                },
                "logoPath": {
                    "type": "string",
                    "default": "",
                    "title": "The logoPath Schema",
                    "examples": [
                        ""
                    ]
                },
                "passwordHash": {
                    "type": "string",
                    "default": "",
                    "title": "The passwordHash Schema",
                    "examples": [
                        ""
                    ]
                },
                "tokens": {
                    "type": "array",
                    "default": [],
                    "title": "The tokens Schema",
                    "items": {},
                    "examples": [
                        []
                    ]
                }
            },
            "examples": [
                {
                    "name": "Donation Tracker",
                    "databaseURL": "mongodb://localhost:27017",
                    "homepage": "https://google.com",
                    "logoPath": "",
                    "passwordHash": "",
                    "tokens": []
                }
            ]
        },
        "legal": {
            "type": "object",
            "default": {},
            "title": "The legal Schema",
            "required": [
                "privacyPolicy",
                "sweepstakesRules"
            ],
            "properties": {
                "privacyPolicy": {
                    "type": "string",
                    "default": "",
                    "title": "The privacyPolicy Schema",
                    "examples": [
                        ""
                    ]
                },
                "sweepstakesRules": {
                    "type": "string",
                    "default": "",
                    "title": "The sweepstakesRules Schema",
                    "examples": [
                        ""
                    ]
                }
            },
            "examples": [
                {
                    "privacyPolicy": "",
                    "sweepstakesRules": ""
                }
            ]
        },
        "frontend": {
            "type": "object",
            "default": {},
            "title": "The frontend Schema",
            "required": [
                "useDefault",
                "donationSuccessMessage"
            ],
            "properties": {
                "useDefault": {
                    "type": "boolean",
                    "default": false,
                    "title": "The useDefault Schema",
                    "examples": [
                        true
                    ]
                },
                "donationSuccessMessage": {
                    "type": "string",
                    "default": "",
                    "title": "The donationSuccessMessage Schema",
                    "examples": [
                        "Thanks for donating! Enjoy the event!"
                    ]
                }
            },
            "examples": [
                {
                    "useDefault": true,
                    "donationSuccessMessage": "Thanks for donating! Enjoy the event!"
                }
            ]
        }
    },
    "examples": [
        {
            "baseURL": "localhost",
            "port": 8080,
            "ssl": {
                "enabled": false,
                "keyPath": "",
                "certPath": ""
            },
            "paypal": {
                "useSandbox": false,
                "currency": "USD"
            },
            "tracker": {
                "name": "Donation Tracker",
                "databaseURL": "mongodb://localhost:27017",
                "homepage": "https://google.com",
                "logoPath": "",
                "passwordHash": "",
                "tokens": []
            },
            "legal": {
                "privacyPolicy": "",
                "sweepstakesRules": ""
            },
            "frontend": {
                "useDefault": true,
                "donationSuccessMessage": "Thanks for donating! Enjoy the event!"
            }
        }
    ]
}