{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {
        "baseURL": "localhost",
        "port": 8080,
        "ssl": {
            "enabled": false,
            "keyPath": "",
            "certPath": ""
        },
        "paypal": {
            "useSandbox": false,
            "currency": "USD"
        },
        "tracker": {
            "name": "Donation Tracker",
            "databaseURL": "mongodb://localhost:27017",
            "logoPath": "",
            "passwordHash": "some_random_string_here",
            "tokens": []
        }
    },
    "title": "Root Schema",
    "required": [
        "baseURL",
        "port",
        "ssl",
        "paypal",
        "tracker"
    ],
    "properties": {
        "baseURL": {
            "type": "string",
            "default": "",
            "title": "The baseURL Schema",
            "examples": [
                "localhost"
            ]
        },
        "port": {
            "type": "integer",
            "default": 0,
            "title": "The port Schema",
            "examples": [
                8080
            ]
        },
        "ssl": {
            "type": "object",
            "default": {},
            "title": "The ssl Schema",
            "required": [
                "enabled",
                "keyPath",
                "certPath"
            ],
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "default": false,
                    "title": "The enabled Schema",
                    "examples": [
                        false
                    ]
                },
                "keyPath": {
                    "type": "string",
                    "default": "",
                    "title": "The keyPath Schema",
                    "examples": [
                        ""
                    ]
                },
                "certPath": {
                    "type": "string",
                    "default": "",
                    "title": "The certPath Schema",
                    "examples": [
                        ""
                    ]
                }
            },
            "examples": [{
                "enabled": false,
                "keyPath": "",
                "certPath": ""
            }]
        },
        "paypal": {
            "type": "object",
            "default": {},
            "title": "The paypal Schema",
            "required": [
                "useSandbox",
                "currency"
            ],
            "properties": {
                "useSandbox": {
                    "type": "boolean",
                    "default": false,
                    "title": "The useSandbox Schema",
                    "examples": [
                        false
                    ]
                },
                "currency": {
                    "type": "string",
                    "default": "",
                    "title": "The currency Schema",
                    "examples": [
                        "USD"
                    ]
                }
            },
            "examples": [{
                "useSandbox": false,
                "currency": "USD"
            }]
        },
        "tracker": {
            "type": "object",
            "default": {},
            "title": "The tracker Schema",
            "required": [
                "name",
                "databaseURL",
                "logoPath",
                "passwordHash",
                "tokens"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "default": "",
                    "title": "The name Schema",
                    "examples": [
                        "Donation Tracker"
                    ]
                },
                "databaseURL": {
                    "type": "string",
                    "default": "",
                    "title": "The databaseURL Schema",
                    "examples": [
                        "mongodb://localhost:27017"
                    ]
                },
                "logoPath": {
                    "type": "string",
                    "default": "",
                    "title": "The logoPath Schema",
                    "examples": [
                        ""
                    ]
                },
                "passwordHash": {
                    "type": "string",
                    "default": "",
                    "title": "The passwordHash Schema",
                    "examples": [
                        "some_random_string_here"
                    ]
                },
                "tokens": {
                    "type": "array",
                    "default": [],
                    "title": "The tokens Schema",
                    "items": {},
                    "examples": [
                        []
                    ]
                }
            },
            "examples": [{
                "name": "Donation Tracker",
                "databaseURL": "mongodb://localhost:27017",
                "logoPath": "",
                "passwordHash": "some_random_string_here",
                "tokens": []
            }]
        }
    },
    "examples": [{
        "baseURL": "localhost",
        "port": 8080,
        "ssl": {
            "enabled": false,
            "keyPath": "",
            "certPath": ""
        },
        "paypal": {
            "useSandbox": false,
            "currency": "USD"
        },
        "tracker": {
            "name": "Donation Tracker",
            "databaseURL": "mongodb://localhost:27017",
            "logoPath": "",
            "passwordHash": "some_random_string_here",
            "tokens": []
        }
    }]
}